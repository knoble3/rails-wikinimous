<div class="container text-justify">
  <div class="row">
    <div class="col-xs-12">
      <% @articles.each do |article| %>
        <h3><%= link_to article.title, article_path(article), class: "article-primary" %></h3>
        <p><%= article.content %></p>
      <% end %>
    </div>
  </div>
</div>
