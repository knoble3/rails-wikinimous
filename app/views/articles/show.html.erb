
<div class="container text-justify">
  <div class="row">
    <div class="col-xs-12">
      <h1><%= @article.title %></h1>
      <p><%= @article.content %></p>
      <br>
      <ul class="list-inline">
        <li><%= link_to "Back", articles_path %></li>
        <li><%= link_to "Edit", edit_article_path(@article) %></li>
        <li><%= link_to "Delete", article_path(@article), method: :delete %></li>
      </ul>
    </div>
  </div>
</div>
